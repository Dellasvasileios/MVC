{
  "name": "element/mvc",
  "description": "MVC PHP Project",
  "type": "project",
  "require": {
    "php": ">=8.1"
  },
  "autoload": {
    "psr-4": {
      "Framework\\": "Framework/",
      "Controllers\\": "Controllers/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5"
  }
}
